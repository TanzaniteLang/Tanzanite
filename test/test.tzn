fun printf(format: Char*, ...): Int
end

fun scanf(format: Char*, ...): Int
end

fun main(): Int
  a: Int
  b: Int

  printf("Write first number: \n")
  scanf("%d", &a)

  printf("Write second number: \n")
  scanf("%d", &b)

  printf("a + b = c\n")
  printf("%d + %d = %d\n", a, b, a + b)
  return 0
end
